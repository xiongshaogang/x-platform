<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
                        "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">
<taglib>
	<tlib-version>1.0</tlib-version>
	<jsp-version>1.2</jsp-version>
	<short-name>t</short-name>
	<uri>/easyui-tags</uri>
	<display-name>"自定义标签"</display-name>
	<tag>
		<name>base</name>
		<tag-class>com.xplatform.base.framework.tag.core.easyui.BaseTag</tag-class>
		<body-content>jsp</body-content>
		<description>父类标签生成JS CSS</description>
		<attribute>
			<name>type</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>可用类型: [base_css|系统所需基本的css文件,easyui、bootstrap等] [base_js|系统所需基本的js文件] [codemirror|代码高亮编辑插件] [ckeditor|web富文本编辑器]</description>
		</attribute>
	</tag>
	<tag>
		<name>datagrid</name>
		<tag-class>com.xplatform.base.framework.tag.core.easyui.DataGridTag</tag-class>
		<body-content>jsp</body-content>
		<description>数据列表</description>
		<attribute>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>列表TABLE标示</description>
		</attribute>
		<attribute>
			<name>treegrid</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是否是树形列表 值为true 或者false</description>
		</attribute>
		<attribute>
			<name>actionUrl</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>分页提交的路径</description>
		</attribute>
		<attribute>
			<name>pagination</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是否分页 true,false</description>
		</attribute>
		<attribute>
			<name>title</name>
			<rtexprvalue>true</rtexprvalue>
			<description>表格标题</description>
		</attribute>
		<attribute>
			<name>idField</name>
			<rtexprvalue>true</rtexprvalue>
			<description>主键字段</description>
		</attribute>
		<attribute>
			<name>width</name>
			<rtexprvalue>true</rtexprvalue>
			<description>表格宽度</description>
		</attribute>
		<attribute>
			<name>height</name>
			<rtexprvalue>true</rtexprvalue>
			<description>表格高度</description>
		</attribute>
		<attribute>
			<name>checkbox</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是否显示复选框</description>
		</attribute>
		<attribute>
			<name>editable</name>
			<rtexprvalue>true</rtexprvalue>
			<description>表格是否可编辑</description>
		</attribute>
		<attribute>
			<name>defaultOpt</name>
			<rtexprvalue>true</rtexprvalue>
			<description>编辑表格默认操作</description>
		</attribute>
		<attribute>
			<name>fit</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是否适应父容器,true 默认 ,false</description>
		</attribute>
		<attribute>
			<name>sortName</name>
			<rtexprvalue>true</rtexprvalue>
			<description>定义的列进行排序</description>
		</attribute>
		<attribute>
			<name>sortOrder</name>
			<rtexprvalue>true</rtexprvalue>
			<description>定义列的排序顺序，只能是"递增 asc"或"降序desc"</description>
		</attribute>
		<attribute>
			<name>fitColumns</name>
			<rtexprvalue>true</rtexprvalue>
			<description>当为true时，自动展开/合同列的大小，以适应的宽度，防止横向滚动</description>
		</attribute>
		<attribute>
			<name>showPageList</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是否显示页面列表</description>
		</attribute>
		<attribute>
			<name>showRefresh</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是否显示刷新按钮</description>
		</attribute>
		<attribute>
			<name>showText</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是否显示分页信息</description>
		</attribute>
		<attribute>
			<name>style</name>
			<rtexprvalue>true</rtexprvalue>
			<description>表格样式easyui,datatables</description>
		</attribute>
		<attribute>
			<name>pageSize</name>
			<rtexprvalue>true</rtexprvalue>
			<description>每页显示条数</description>
		</attribute>
		<attribute>
			<name>onLoadSuccess</name>
			<rtexprvalue>true</rtexprvalue>
			<description>数据加载成调用方法</description>
		</attribute>
		<attribute>
			<name>onDblClickRow</name>
			<rtexprvalue>true</rtexprvalue>
			<description>双击行调用方法</description>
		</attribute>
		<attribute>
			<name>onClick</name>
			<rtexprvalue>true</rtexprvalue>
			<description>单击调用方法 tree 下function(rowData),dataGrid
				下function(rowIndex,rowData)</description>
		</attribute>
		<attribute>
			<name>queryMode</name>
			<rtexprvalue>true</rtexprvalue>
			<description>查询模式：single(单条件查询：默认),group(组合查询)</description>
		</attribute>
		<attribute>
			<name>autoLoadData</name>
			<rtexprvalue>true</rtexprvalue>
			<description>列表页面数据加载模式。true自动加载数据，false手动加载，默认为true</description>
		</attribute>
		<attribute>
			<name>openFirstNode</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是不是展开第一个节点,在树形情况下,true展开,false不展开默认false</description>
		</attribute>
		<attribute>
			<name>entityName</name>
			<rtexprvalue>true</rtexprvalue>
			<description>对应的实体对象,如果entity和controller都是规则的可以不填,自动补全标签做关联</description>
		</attribute>
		<attribute>
			<name>rowStyler</name>
			<rtexprvalue>true</rtexprvalue>
			<description>行 css函数 指定名称就可以,调用为 functionName(index,row)这样调用</description>
		</attribute>
		<attribute>
			<name>extendParams</name>
			<rtexprvalue>true</rtexprvalue>
			<description>datagrid 的扩展字段,如果easyui上面有但是jeecg没有这个属性可以自己添加
				添加规则和easyui的field一致</description>
		</attribute>
		<attribute>
			<name>showButtonDiv</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是否显示列表上方按钮Div</description>
		</attribute>
		<attribute>
			<name>statistics</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是否是统计功能使用的datagrid(查询框的name会变化)</description>
		</attribute>
		<attribute>
			<name>compareSign</name>
			<rtexprvalue>true</rtexprvalue>
			<description>比较符(用于给查询框的name 改造成 filter_比较符_字段名 的形式)</description>
		</attribute>
		<attribute>
			<name>view</name>
			<rtexprvalue>true</rtexprvalue>
			<description>datagrid视图类型</description>
		</attribute>
		<attribute>
			<name>groupField</name>
			<rtexprvalue>true</rtexprvalue>
			<description>datagrid分组字段</description>
		</attribute>
		<attribute>
			<name>groupFormatter</name>
			<rtexprvalue>true</rtexprvalue>
			<description>//datagrid分组函数,返回分组栏上的内容,范例groupFormatter="test(value,rows)" 自己去定义test方法 返回一段字符串</description>
		</attribute>
		<attribute>
			<name>showButtonDiv</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是否显示列表上方操作按钮Div</description>
		</attribute>
		<attribute>
			<name>showOptColumn</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是否有显示"操作"那一列</description>
		</attribute>
		<attribute>
			<name>flag</name>
			<rtexprvalue>true</rtexprvalue>
			<description>页面中有多个datagrid时,需要用jquery统一取到其中所有的或某几个datagrid,可以用此属性</description>
		</attribute>
		<attribute>
			<name>pageNumber</name>
			<rtexprvalue>true</rtexprvalue>
			<description>在设置分页属性的时候初始化页码。</description>
		</attribute>
		<attribute>
			<name>resizeHandle</name>
			<rtexprvalue>true</rtexprvalue>
			<description>调整列的位置，可用的值有：'left','right','both'。在使用'right'的时候用户可以通过拖动右侧边缘的列标题调整列，等等。</description>
		</attribute>
		<attribute>
			<name>autoRowHeight</name>
			<rtexprvalue>true</rtexprvalue>
			<description>定义设置行的高度，根据该行的内容。设置为false可以提高负载性能。</description>
		</attribute>
		<attribute>
			<name>striped</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是否显示斑马线效果。</description>
		</attribute>
		<attribute>
			<name>method</name>
			<rtexprvalue>true</rtexprvalue>
			<description>该方法类型请求远程数据。</description>
		</attribute>
		<attribute>
			<name>nowrap</name>
			<rtexprvalue>true</rtexprvalue>
			<description>如果为true，则在同一行中显示数据。设置为true可以提高加载性能。</description>
		</attribute>
		<attribute>
			<name>loadMsg</name>
			<rtexprvalue>true</rtexprvalue>
			<description>在从远程站点加载数据的时候显示提示消息。</description>
		</attribute>
		<attribute>
			<name>rownumbers</name>
			<rtexprvalue>true</rtexprvalue>
			<description>如果为true，则显示一个行号列。</description>
		</attribute>
		<attribute>
			<name>singleSelect</name>
			<rtexprvalue>true</rtexprvalue>
			<description>如果为true，则只允许选择一行。</description>
		</attribute>
		<attribute>
			<name>checkOnSelect</name>
			<rtexprvalue>true</rtexprvalue>
			<description>如果为true,当用户点击行的时候该复选框就会被选中或取消选中;如果为false,当用户仅在点击该复选框的时候才会呗选中或取消。</description>
		</attribute>
		<attribute>
			<name>selectOnCheck</name>
			<rtexprvalue>true</rtexprvalue>
			<description>如果为true,单击复选框将永远选择行;如果为false，选择行将不选中复选框。</description>
		</attribute>
		<attribute>
			<name>remoteSort</name>
			<rtexprvalue>true</rtexprvalue>
			<description>定义从服务器对数据进行排序。</description>
		</attribute>
		<attribute>
			<name>showHeader</name>
			<rtexprvalue>true</rtexprvalue>
			<description>定义是否显示行头。</description>
		</attribute>
		<attribute>
			<name>showFooter</name>
			<rtexprvalue>true</rtexprvalue>
			<description>定义是否显示行脚。</description>
		</attribute>
		<attribute>
			<name>scrollbarSize</name>
			<rtexprvalue>true</rtexprvalue>
			<description>滚动条的宽度(当滚动条是垂直的时候)或高度(当滚动条是水平的时候)。</description>
		</attribute>
		<attribute>
			<name>animate</name>
			<rtexprvalue>true</rtexprvalue>
			<description>定义在节点展开或折叠的时候是否显示动画效果。</description>
		</attribute>
		<attribute>
			<name>exportParams</name>
			<rtexprvalue>true</rtexprvalue>
			<description>导出所需参数,可以直接设置,也可以通过t:dgExportParams设置(url参数形式,形如:<![CDATA[&title=人员列表&titleHeight=5]]>)</description>
		</attribute>
		<attribute>
			<name>border</name>
			<rtexprvalue>true</rtexprvalue>
			<description>列表是否需要边框</description>
		</attribute>
	</tag>
	<tag>
		<name>dgExportParams</name>
		<tag-class>com.xplatform.base.framework.tag.core.easyui.DataGridExportParamsTag</tag-class>
		<body-content>jsp</body-content>
		<description>列表提供导出所需参数标签</description>
		<attribute>
			<name>title</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>第一行大标题名称</description>
		</attribute>
		<attribute>
			<name>entityClass</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>注解配置的实体</description>
		</attribute>
		<attribute>
			<name>titleHeight</name>
			<rtexprvalue>true</rtexprvalue>
			<description>第一行大标题高度(默认20)</description>
		</attribute>
		<attribute>
			<name>titleFontSize</name>
			<rtexprvalue>true</rtexprvalue>
			<description>第一行大标题大小(默认24)</description>
		</attribute>
		<attribute>
			<name>secondTitle</name>
			<rtexprvalue>true</rtexprvalue>
			<description>第二行标题名称</description>
		</attribute>
		<attribute>
			<name>secondTitleHeight</name>
			<rtexprvalue>true</rtexprvalue>
			<description>第二行标题高度(默认8)</description>
		</attribute>
		<attribute>
			<name>secondTitleFontSize</name>
			<rtexprvalue>true</rtexprvalue>
			<description>第二行标题大小</description>
		</attribute>
		<attribute>
			<name>sheetName</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>sheet名称</description>
		</attribute>
		<attribute>
			<name>exclusions</name>
			<rtexprvalue>true</rtexprvalue>
			<description>配置了注解,但依然过滤不进行导入/出的属性,形如(username,age,sex)</description>
		</attribute>
		<attribute>
			<name>color</name>
			<rtexprvalue>true</rtexprvalue>
			<description>第一行大标题底色(值类型为short,需查api获得颜色对应,例如:9对应HSSFColor.WHITE.index 默认)</description>
		</attribute>
		<attribute>
			<name>secondColor</name>
			<rtexprvalue>true</rtexprvalue>
			<description>第二行标题底色(例如:9对应HSSFColor.WHITE.index 默认</description>
		</attribute>
		<attribute>
			<name>headerColor</name>
			<rtexprvalue>true</rtexprvalue>
			<description>表头属性行的颜色(例如:40对应HSSFColor.SKY_BLUE.index 默认)</description>
		</attribute>
		<attribute>
			<name>dataHanlder</name>
			<rtexprvalue>true</rtexprvalue>
			<description>数据处理接口,以此为主,replace,format都在这后面</description>
		</attribute>
		<attribute>
			<name>needHandlerFields</name>
			<rtexprvalue>true</rtexprvalue>
			<description>需要用自定义Handler类处理的字段,形如(username,age,sex)</description>
		</attribute>
		<attribute>
			<name>fileName</name>
			<rtexprvalue>true</rtexprvalue>
			<description>导出的文件名</description>
		</attribute>
		<attribute>
			<name>exportServer</name>
			<rtexprvalue>true</rtexprvalue>
			<description>使用的导出方法类(可使用自定义类,完成更灵活的配置)</description>
		</attribute>
	</tag>
	<tag>
		<name>dgImportParams</name>
		<tag-class>com.xplatform.base.framework.tag.core.easyui.DataGridImportParamsTag</tag-class>
		<body-content>jsp</body-content>
		<description>列表提供excel导入所需参数标签</description>
		<attribute>
			<name>titleRows</name>
			<rtexprvalue>true</rtexprvalue>
			<description>标题行数,默认0</description>
		</attribute>
		<attribute>
			<name>headRows</name>
			<rtexprvalue>true</rtexprvalue>
			<description>表头属性行数,默认1</description>
		</attribute>
		<attribute>
			<name>startRows</name>
			<rtexprvalue>true</rtexprvalue>
			<description>字段真正值和列标题之间的距离 默认0(暂未使用)</description>
		</attribute>
		<attribute>
			<name>startCell</name>
			<rtexprvalue>true</rtexprvalue>
			<description>需导入数据的开始列</description>
		</attribute>
		<attribute>
			<name>endCell</name>
			<rtexprvalue>true</rtexprvalue>
			<description>需导入数据的结束列</description>
		</attribute>
		<attribute>
			<name>keyIndex</name>
			<rtexprvalue>true</rtexprvalue>
			<description>主键设置,如何这个cell没有值,就跳过 或者认为这个是list的下面的值</description>
		</attribute>
		<attribute>
			<name>sheetNum</name>
			<rtexprvalue>true</rtexprvalue>
			<description>上传表格需要读取的sheet数量,默认为1</description>
		</attribute>
		<attribute>
			<name>needSave</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是否需要保存上传的Excel,默认为false</description>
		</attribute>
		<attribute>
			<name>dataHanlder</name>
			<rtexprvalue>true</rtexprvalue>
			<description>数据处理接口,以此为主,replace,format都在这后面</description>
		</attribute>
		<attribute>
			<name>needHandlerFields</name>
			<rtexprvalue>true</rtexprvalue>
			<description>需要用自定义Handler类处理的字段,形如(username,age,sex)</description>
		</attribute>
		<attribute>
			<name>templateCode</name>
			<rtexprvalue>true</rtexprvalue>
			<description>下载模板code</description>
		</attribute>
		<attribute>
			<name>name</name>
			<rtexprvalue>true</rtexprvalue>
			<description>导入结果集名称(用于设、取ehcache缓存中数据)</description>
		</attribute>
		<attribute>
			<name>submitUrl</name>
			<rtexprvalue>true</rtexprvalue>
			<description>自行处理结果集的请求url(比如拿到结果集保存)</description>
		</attribute>
		<attribute>
			<name>entityClass</name>
			<rtexprvalue>true</rtexprvalue>
			<description>注解配置的实体</description>
		</attribute>
	</tag>
	<tag>
		<name>dgCol</name>
		<tag-class>com.xplatform.base.framework.tag.core.easyui.DataGridColumnTag</tag-class>
		<body-content>jsp</body-content>
		<description>列表字段</description>
		<attribute>
			<name>title</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>字段标题</description>
		</attribute>
		<attribute>
			<name>field</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>字段值</description>
		</attribute>
		<attribute>
			<name>width</name>
			<rtexprvalue>true</rtexprvalue>
			<description>字段宽度</description>
		</attribute>
		<attribute>
			<name>rowspan</name>
			<rtexprvalue>true</rtexprvalue>
			<description>字段跨列</description>
		</attribute>
		<attribute>
			<name>colspan</name>
			<rtexprvalue>true</rtexprvalue>
			<description>字段跨行</description>
		</attribute>
		<attribute>
			<name>align</name>
			<rtexprvalue>true</rtexprvalue>
			<description>数据对齐方式，可选值有left,right,center</description>
		</attribute>
		<attribute>
			<name>halign</name>
			<rtexprvalue>true</rtexprvalue>
			<description>猎头对齐方式，可选值有left,right,center</description>
		</attribute>
		<attribute>
			<name>sortable</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是否排序</description>
		</attribute>
		<attribute>
			<name>checkbox</name>
			<rtexprvalue>true</rtexprvalue>
			<description>是否显示选择框</description>
		</attribute>
		<attribute>
			<name>sql</name>
			<rtexprvalue>true</rtexprvalue>
			<description>code替换name的sql</description>
		</attribute>
		<attribute>
			<name>formatter</name>
			<rtexprvalue>true</rtexprvalue>
			<description>自定义函数名称</description>
		</attribute>
		<attribute>
			<name>precision</name>
			<rtexprvalue>true</rtexprvalue>
			<description>如果该列是小数类型(比如金额),此字段表示格式化到小数点后几位,含四舍五入逻辑(比如原值,32.4566,t:dgCol不指定该值,依然是32.4566,如果precision="2",则显示值为32.46)</description>
		</attribute>
		<attribute>
			<name>hidden</name>
			<rtexprvalue>true</rtexprvalue>
			<description>列隐藏(隐藏:false)</description>
		</attribute>
		<attribute>
			<name>replace</name>
			<rtexprvalue>true</rtexprvalue>
			<description>字段值替换(格式 ：old_new,old2_new2)</description>
		</attribute>
